{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.17",
    "symfony/dotenv": "*",
    "symfony/framework-bundle": "^6.4",
    "symfony/monolog-bundle": "^3.10",
    "symfony/runtime": "^7.4",
    "symfony/serializer": "^6.4",
    "symfony/validator": "^6.4",
    "symfony/yaml": "^6.4"
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.60",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php bin/console cache:clear"
    ],
    "post-update-cmd": [
      "@php bin/console cache:clear"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  }
}
